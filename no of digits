//This program is to count the total number of digits.

import java.util.*;
class NoOfDigit
{
public static void main(String ar[])
{
int a,count=0;
System.out.println("Enter the number: ");
Scanner s=new Scanner(System.in);
a=s.nextInt;
while(a!=0)
{
a/=10;
++count;
}
System.out.println("The no of digits are: "+count);
}
}
