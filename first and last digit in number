// This program is to find the first and last digit of the given number

import java.util.*;
class FirstLast
{
public static void main(String ar[])
{
int a,first,last,rem=0;
System.out.println("Enter the number: ");
Scanner s= new Scanner(System.in);
a=s.nextInt();
last=a%10;
for(;a!=0;)
{
rem=a%10;
a/=10;
}
first=rem;
System.out.println("The first digit is: "+first+"The last digit is: "+last);
}
}
